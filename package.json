{
  "name": "onelineReader",
  "version": "1.0.0",
  "description": "My slick Nuxt.js project",
  "author": "chige12",
  "private": true,
  "scripts": {
    "dev": "processmd contents/posts/*.md --stdout --outputDir contents/json > contents/summary.json && nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "processmd contents/posts/*.md --stdout --outputDir contents/json > contents/summary.json && nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/markdownit": "^1.2.5",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "processmd": "^4.4.2",
    "sass-loader": "^7.1.0"
  }
}
